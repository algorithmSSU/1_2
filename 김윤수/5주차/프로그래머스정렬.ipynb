    "def solution(citations):\n",
    "    citations.sort()\n",
    "    citLen = len(citations)\n",
    "    for idx, value in enumerate(citations):\n",
    "        if value  >= citLen - idx:\n",
    "            return citLen - idx\n",
    "    return 0"
   ]
